#include <iostream>
using namespace std;

int main() {
	int n; bool judge = false;
	scanf("%d", &n);
	if (n == 1) {
		judge = true;
	}
	else{
		while (1) {
			if (n == 2) {
				judge = true;
				break;
			}
			if (n % 2 == 0) {
				n = n / 2;
			}
			else {
				break;
			}
		}
	}
	printf("%d\n", judge);
	return 0;
}
